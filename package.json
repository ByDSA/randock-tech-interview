{
  "name": "randock",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "type": "commonjs",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "clean:modules": "rm -rf node_modules",
    "lint:eslint": "eslint **/*.{ts,js,tsx,jsx}",
    "lint:prettier": "prettier **/*.{json,jsonc,scss,yml}",
    "lint:fix": "pnpm run lint:prettier --write && pnpm run lint:eslint --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "pnpm run test --watch",
    "test:cov": "pnpm run test --coverage",
    "test:cov:clean": "rm -rf coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:onlyLocal": "pnpm run test --testPathIgnorePatterns=.e2e.",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-express": "10.1.3",
    "@nestjs/swagger": "7.1.8",
    "axios": "1.4.0",
    "cache-manager": "^5.2.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "helmet": "^7.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.11",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.5.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.47.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.2",
    "prettier": "3.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "husky": "8.0.3"
  }
}
